{
  "manifest_version": 2,

  "name": "Eye",
  "description": "Eye Description",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content_script.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://ajax.googleapis.com/"
  ],
  "description": "Eye Description",
  "background": {
    "scripts": ["jquery.js", "background.js"]
  }
}
