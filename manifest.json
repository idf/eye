{
  "manifest_version": 2,

  "name": "Eye",
  "description": "Eye Description",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
	  "16" : "icon_black.png",
		"24" : "icon_black.png",
		"32" : "icon_black.png",
		"128": "icon_black.png"
	},
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content_script.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": ["jquery.js", "background.js"]
  }
}
